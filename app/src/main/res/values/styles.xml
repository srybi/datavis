<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="VerticalDivider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginStart">6dp</item>
        <item name="android:layout_marginEnd">6dp</item>
        <item name="android:background">#86BCB7B7</item>
    </style>

    <style name="MyTooltip" parent="Widget.MaterialComponents.Tooltip">
        <item name="backgroundTint">@color/black</item>
        <item name="android:textAppearance">@style/MyTooltip.TextAppearance</item>
    </style>

    <style name="MyTooltip.TextAppearance" parent="@style/TextAppearance.MaterialComponents.Tooltip">
        <item name="android:textColor">@color/colorEricsson_White</item>
    </style>

    <style name="ApplyButton" parent="Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">@color/colorEricsson_Blue</item>
        <item name="android:textColor">@color/colorEricsson_White</item>
    </style>

</resources>