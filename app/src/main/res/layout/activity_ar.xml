<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ARActivity"
    android:orientation="vertical">



    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/arFragment"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" />
    <TextView
        android:id="@+id/meta_HHPBW_deg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="@string/meta_NF"
        android:background="@color/black_20"
        android:textColor="@color/white"/>
    <TextView
        android:id="@+id/meta_VHPBW_deg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/meta_HHPBW_deg"
        android:text="@string/meta_NF"
        android:background="@color/black_20"
        android:textColor="@color/white"/>
    <TextView
        android:id="@+id/meta_Directivity_dBi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/meta_VHPBW_deg"
        android:text="@string/meta_NF"
        android:background="@color/black_20"
        android:textColor="@color/white"/>


</RelativeLayout>